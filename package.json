{
  "name": "Mirrors",
  "version": "v0.7.1",
  "author": "jonkofee <jonkofee@icloud.com>, klimash <andrey.klimash@gmail.com>",
  "description": "Vue.js SPA for Mirror's Photo",
  "license": null,
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MirrorsPhoto/electron"
  },
  "scripts": {
    "dev": "webpack && webpack-dev-server --inline --hot",
    "build": "rimraf ./dist && webpack && electron-builder",
    "release": "rimraf ./dist && webpack && electron-builder -p always"
  },
  "build": {
    "productName": "Mirror's Photo",
    "appId": "org.mirrors-photo.electron-vue",
    "directories": {
      "output": "app"
    },
    "files": [
      "dist/**/*"
    ],
    "mac": {
      "category": "public.app-category.business",
      "icon": "src/icons/icon.icns",
      "target": [
        "zip"
      ]
    }
  },
  "dependencies": {
    "axios": "^0.17.0",
    "countup.js": "^1.9.2",
    "electron-updater": "^2.13.0",
    "jwt-decode": "^2.2.0",
    "online-js": "^1.0.2",
    "vue": "^2.5.3",
    "vue-electron": "^1.0.6",
    "vuex": "^3.0.1",
    "ws": "^3.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2017": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "electron": "^1.7.9",
    "electron-builder": "^19.45.0",
    "electron-devtools-installer": "^2.2.1",
    "executive": "^1.5.12",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.6.0",
    "sass-loader": "^6.0.6",
    "svg-spritemap-webpack-plugin": "^1.0.3",
    "vue-loader": "^12.2.2",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "babel": {
    "presets": [
      "es2017"
    ]
  }

}
