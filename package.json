{
  "name": "Mirrors",
  "version": "v0.7.1",
  "author": "jonkofee <jonkofee@icloud.com>",
  "description": "Vue.js SPA for Mirror's Photo",
  "license": null,
  "repository": {
    "type": "git",
    "url": "https://github.com/MirrorsPhoto/electron"
  },
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "release": "node .electron-vue/build.js && electron-builder -p always"
  },
  "build": {
    "productName": "Mirror's Photo",
    "appId": "org.mirrors-photo.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.business",
      "icon": "electron/icons/icon.icns",
      "target": [
        "zip"
      ]
    },
    "win": {
      "icon": "electron/icons/icon.ico"
    },
    "linux": {
      "icon": "electron/icons"
    }
  },
  "dependencies": {
    "axios": "^0.17.0",
    "countup.js": "^1.9.2",
    "electron-updater": "^2.13.0",
    "jwt-decode": "^2.2.0",
    "online-js": "^1.0.1",
    "vue": "^2.5.3",
    "vue-electron": "^1.0.6",
    "vuex": "^3.0.1",
    "ws": "^3.3.0"
  },
  "devDependencies": {
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.5",
    "electron-builder": "^19.37.2",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "multispinner": "^0.2.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svg-spritemap-webpack-plugin": "^1.0.3",
    "url-loader": "^0.6.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.2.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
